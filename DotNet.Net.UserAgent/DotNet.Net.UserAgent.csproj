<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1;net5.0;net6.0</TargetFrameworks>
    <LangVersion>latest</LangVersion>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>System.Net</RootNamespace>
    <PackageId>MikaelDui.Net.UserAgent</PackageId>
    <Authors>Mikael DÃºi Bolinder</Authors>
    <Product>MikaelDui.Net.UserAgent</Product>
    <Description>Adds a User-Agent class to System.Net that can be used with HttpClient and ClientWebSocket.</Description>
    <PackageProjectUrl>https://github.com/mikaeldui/user-agent-dotnet</PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageIcon>DotNet.Net.UserAgent.png</PackageIcon>
    <RepositoryUrl>https://github.com/mikaeldui/user-agent-dotnet</RepositoryUrl>
    <PackageTags>UserAgent; HttpClient; Http; WebSockets</PackageTags>
    <PackageLicenseExpression>Unlicense</PackageLicenseExpression>
    <AssemblyName>MikaelDui.Net.UserAgent</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\DotNet.Net.UserAgent.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
